{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  },
  "Download": {
    "MaxConcurrentDownloads": 3,
    "MaxSegmentsPerDownload": 8,
    "BufferSize": 8192,
    "RetryDelaySeconds": 5,
    "MaxRetries": 3,
    "TempDirectory": "./Downloads/Temp",
    "MetadataDirectory": "./Downloads/Metadata",
    "ProgressUpdateIntervalMs": 500,
    "MinFileSizeForSegmentation": 1048576
  }
}
